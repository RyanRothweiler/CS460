
@{
    ViewBag.Title = "AddRequests";
}

<h2>Change of Major/Minor/Advisor Form</h2>
<h1></h1>

<div class="container">
    <form method="get">
        <div class="form-group row">
            <label for="lastname" class="col-sm-2 col-form-label">Last Name</label>
            <div class="col-sm-10">
                <input id="lastname" class="form-control" type="text" name="LastName" value="last name" />
            </div>
        </div>

        <div class="form-group row">
            <label for="firstname" class="col-sm-2 col-form-label">First Name</label>
            <div class="col-sm-10">
                <input id="firstname" class="form-control" type="text" name="FirstName" value="first name" />
            </div>
        </div>

        <div class="form-group row">
            <label for="date" class="col-sm-2 col-form-label">Date</label>
            <div class="col-sm-10">
                <input id="date" class="form-control" type="date" name="Date" value="first name" />
            </div>
        </div>

        <div class="form-group row">
            <label for="phone" class="col-sm-2 col-form-label">Phone Number</label>
            <div class="col-sm-10">
                <input id="phone" class="form-control" type="tel" name="Phone" value="phone Num" />
            </div>
        </div>

        <div class="form-group row">
            <label for="catYear" class="col-sm-2 col-form-label">Catalog Year</label>
            <div class="col-sm-10">
                <input id="catYear" class="form-control" type="number" name="CatalogYear" value="first name" />
            </div>
        </div>

        <div class="form-group row">
            <label for="firstname" class="col-sm-2 col-form-label">First Name</label>
            <div class="col-sm-10">
                <input id="firstname" class="form-control" type="text" name="FirstName" value="first name" />
            </div>
        </div>

        <div class="form-group row">
            <label for="vnum" class="col-sm-2 col-form-label">V Number</label>
            <div class="col-sm-10">
                <input id="vnum" class="form-control" type="number" name="VNumber" value="v number" />
            </div>
        </div>

        <div class="form-group row">
            <label for="email" class="col-sm-2 col-form-label">E-Mail</label>
            <div class="col-sm-10">
                <input id="email" class="form-control" type="email" name="EMail" value="email" />
            </div>
        </div>

        <div class="form-group row">
            <label for="major" class="col-sm-2 col-form-label">Major</label>
            <div class="col-sm-10">
                <input id="major" class="form-control" type="text" name="Major" value="major" />
            </div>
        </div>

        <div class="form-group row">
            <label for="minor" class="col-sm-2 col-form-label">Minor</label>
            <div class="col-sm-10">
                <input id="minor" class="form-control" type="text" name="Minor" value="minor" />
            </div>
        </div>

        <div class="form-group row">
            <label for="advisor" class="col-sm-2 col-form-label">Advisor</label>
            <div class="col-sm-10">
                <input id="advisor" class="form-control" type="text" name="Advisor" value="advisor" />
            </div>
        </div>
    </form>
</div>

