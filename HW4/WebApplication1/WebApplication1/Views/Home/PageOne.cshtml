
@{
    ViewBag.Title = "PageOne";
}

<h2>PageOne</h2>

<form method="get">
    <input type="text" name="itemA" value="Your name?" />
    <input type="submit" name="submit" value="Submit" />
</form>

<div class="row">
    <div class="col-md-6">
        <table class="table table-striped">
            @foreach (var s in ViewBag.data)
            {
                <tr><td>@s</td></tr>
            }
        </table>
    </div>
</div>